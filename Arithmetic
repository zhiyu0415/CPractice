#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i,j,sum,s,sd,p,pd,d;
    while(scanf("%d %d",&i,&j)){
          if (i<j){
            int tmp;
            tmp = i;
            i = j;
            j = tmp;
          }
          int sum = i + j;
          int s = (i+j)*(i+j);
          int sd = (i-j)*(i-j);
          int p = (i+j)*(i+j)*(i+j);
          int pd = (i-j)*(i-j)*(i-j);
          int d = i-j;
          printf("%d\n%d\n%d\n%d\n%d\n%d\n",sum,s,p,d,sd,pd);
          }
}
