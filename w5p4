#include <stdio.h>
#include<stdlib.h>
#include<stdbool.h>
#include<math.h>

#define M_PI 3.14159265358979323846264338327

float oval(float,float);

int main() {
	float a, b;
	scanf("%f %f", &a, &b);
	printf("%.3f\n", oval(a, b));
}

float oval(float a, float b) {
	float ans = M_PI* a* b;
	return ans;
}
