#include <stdio.h>
#include<stdlib.h>
#include<stdbool.h>
#include<math.h>

void printrec(int,int);

int main() {
	int a, b;
	scanf("%d %d", &a, &b);
	printrec(a, b);
}

void printrec(int a, int b) {
	for (int i = 0; i < a; i++) {
		for (int j = 0; j < b; j++) {
			printf("#");
		}printf("\n");
	}
}

